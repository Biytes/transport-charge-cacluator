{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/vvip/vvip",
			"style": {
				"navigationBarTitleText": "vvip"
			}
		},
		{
			"path": "pages/vip/vip",
			"style": {
				"navigationBarTitleText": "vip"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		},
		{
			"path": "pages/order/preview",
			"style": {
				"navigationBarTitleText": "订单预览"
			}
		}
	],
	"tabBar": {
        "color": "#717171",
		"selectedColor": "#333333",
        "borderStyle": "white",
		"iconWidth": "24px",
		"backgroundColor": "#fff",
		"display": "block",
        "list": [
			{
                "pagePath": "pages/vvip/vvip",
                "iconPath": "static/images/tab/vvip.png",
                "selectedIconPath": "static/images/tab/vvip-active.png",
                "text": "vvip"
            },
			{
                "pagePath": "pages/vip/vip",
                "iconPath": "static/images/tab/vip.png",
                "selectedIconPath": "static/images/tab/vip-active.png",
                "text": "vip"
            },
            {
                "pagePath": "pages/my/my",
                "iconPath": "static/images/tab/my.png",
                "selectedIconPath": "static/images/tab/my-active.png",
                "text": "my"
            }
        ]
    },
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#F8F8F8"
	}
}
